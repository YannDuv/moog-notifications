{
  "name": "moogsoft-notifications",
  "version": "1.0.0",
  "main": "src/main.ts",
  "author": "Yann Duval",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --open \"Google Chrome\" --config webpack.config.js",
    "test:tu": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' node_modules/.bin/ava-ts",
    "test:tu:watch": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' node_modules/.bin/ava --watch",
    "test:e2e": "node_modules/.bin/cypress open"
  },
  "dependencies": {
    "rxjs": "^6.3.3",
    "uuidjs": "^4.2.1",
    "vue": "2.5.21",
    "vue-rx": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/rx": "^4.1.1",
    "ava": "^1.0.1",
    "ava-ts": "^0.25.2",
    "css-loader": "^2.0.2",
    "cypress": "^3.1.3",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "vue-loader": "15.4.2",
    "vue-template-compiler": "2.5.21",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.13"
  },
  "ava": {
    "files": [
      "src/**/*.spec.ts",
      "tests/unit/**/*.spec.ts"
    ],
    "cache": true,
    "concurrency": 5,
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}